class Solition{
public:
/**
* @param s: The first string 
* @param t: The second string 
@return true or false
*/
bool anagram(string s,string t){
	if(s.empty() || t.empty()){
	return false;
	}
	if (s.size()!=t.size()){
	rerurn false;
	}
	int letterCount[256]={0};
	for (int = 0;i !=t.size(); ++i){
	++letterCount[[i]];
	--lettrtCount[[i]];
	}
	for (int i=0; i !=t.size();++i){
		if (letterCount[[i]] !=0){
		return false;
		}
	}
	return true;
	}
}



}
